{
    "name": "tiktok-scraper-nowatermarks",
    "version": "1.0.2",
    "description": "Wrapper for TikFast to download videos without watermarks",
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "tslint -c tslint.json -p tsconfig.json --fix && npm run format",
        "build": "tsc && ts-node copyStaticAssets.ts",
        "start": "node ./dist/.",
        "dev": "ts-node-dev src/index.ts",
        "start_pitch_scheduler": "node ./dist/scheduler/igPitchScheduler.js",
        "start_pitch_scheduler_dev": "ts-node-dev src/scheduler/igPitchScheduler.ts",
        "test": "jest --config jestconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags",
        "lint": "tslint -c tslint.json -p tsconfig.json --fix",
        "patch": "npm version patch",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/swherden/tiktok-scraper-nowatermarks.git"
    },
    "keywords": [],
    "author": "",
    "license": "OWN",
    "bugs": {
        "url": "https://github.com/swherden/tiktok-scraper-nowatermarks/issues"
    },
    "homepage": "https://github.com/swherden/tiktok-scraper-nowatermarks#readme",
    "devDependencies": {
        "@types/shelljs": "^0.8.11",
        "prettier": "^2.7.1",
        "shelljs": "^0.8.5",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "tslint": "^6.1.3",
        "typescript": "^4.8.3"
    }
}
